{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 9,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "extends": "airbnb",
    "plugins": [
        "react",
        "import"
    ],
    "rules": {
        "arrow-parens": [2, "as-needed"],
        "comma-dangle": ["error", "never"],

        "react/prop-types": 0,
        "react/no-did-mount-set-state": 0,

        "import/no-unresolved": 0,
        "import/extensions": 0
    }
}
